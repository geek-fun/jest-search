    "jest-elasticsearch",
    "elasticsearch local",
    "elasticsearch-local",
    "OpenSearch",
    "jest-opensearch",
    "opensearch local",
    "opensearch-local",
    "ZincSearch",
    "jest-zincsearch",
    "zincsearch local",
    "zincsearch-local",
    "jest environment",
    "jest preset",
    "jest-preset",
    "es",
    "es local"
  ],
  "bugs": {
    "url": "https://github.com/geek-fun/jest-search/issues"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "gunzip-maybe": "^1.4.2",
